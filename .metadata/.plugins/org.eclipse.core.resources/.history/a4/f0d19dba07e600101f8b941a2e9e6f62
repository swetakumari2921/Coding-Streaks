package my.collection;

import java.util.Objects;

public class Ex29Book {
	private String name;
	private String author;
	private double price;
	
	public Ex29Book(String name, String author, double price) {
		this.name = name;
		this.author = author;
		this.price = price;
	}

	@Override
	public String toString() {
		return "Ex29Book [name=" + name + ", author=" + author + ", price=" + price + "]";
	}

	@Override
	public int hashCode() {
		return Objects.hash(author, name, price);
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Ex29Book other = (Ex29Book) obj;
		return Objects.equals(author, other.author) && Objects.equals(name, other.name)
				&& Double.doubleToLongBits(price) == Double.doubleToLongBits(other.price);
	}
	
	
}
